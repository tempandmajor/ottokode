{
  "name": "branchcode-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "cd web-app && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:web\"",
    "build": "NODE_ENV=production vite build",
    "build:dev": "NODE_ENV=development vite build",
    "build:analyze": "NODE_ENV=production ANALYZE=true vite build",
    "build:web": "cd web-app && npm run build",
    "build:all": "npm run build && npm run build:web",
    "build-with-tsc": "tsc && NODE_ENV=production vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "clean": "rm -rf dist web-app/.next node_modules/.vite",
    "type-check": "tsc --noEmit && cd web-app && npm run type-check",
    "lint": "echo 'No linter configured' && cd web-app && npm run lint",
    "test": "echo 'No tests configured'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@google/generative-ai": "^0.24.1",
    "@mistralai/mistralai": "^1.10.0",
    "@monaco-editor/react": "^4.7.0",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "@tauri-apps/plugin-http": "^2.0.0",
    "@tauri-apps/plugin-opener": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.12.1",
    "cohere-ai": "^7.19.0",
    "crypto-js": "^4.2.0",
    "openai": "^5.20.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "stripe": "^18.5.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
