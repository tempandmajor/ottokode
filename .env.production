# Production Environment Configuration
# CRITICAL: Update ALL placeholder values before deploying

# Stripe Configuration (Production)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_REPLACE_WITH_PRODUCTION_KEY
STRIPE_SECRET_KEY=sk_live_REPLACE_WITH_PRODUCTION_SECRET
STRIPE_WEBHOOK_SECRET=whsec_REPLACE_WITH_PRODUCTION_WEBHOOK_SECRET

# Supabase Configuration (Production)
SUPABASE_URL=https://YOUR_PRODUCTION_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=REPLACE_WITH_PRODUCTION_SERVICE_ROLE_KEY
SUPABASE_ANON_KEY=REPLACE_WITH_PRODUCTION_ANON_KEY

# AI Provider Keys (Production - Optional, users can use their own)
OPENAI_API_KEY=sk-REPLACE_WITH_PRODUCTION_OPENAI_KEY
ANTHROPIC_API_KEY=sk-ant-REPLACE_WITH_PRODUCTION_ANTHROPIC_KEY
GOOGLE_AI_API_KEY=REPLACE_WITH_PRODUCTION_GOOGLE_KEY
COHERE_API_KEY=REPLACE_WITH_PRODUCTION_COHERE_KEY
MISTRAL_API_KEY=REPLACE_WITH_PRODUCTION_MISTRAL_KEY

# Frontend AI Keys (Client-side access)
REACT_APP_OPENAI_API_KEY=sk-REPLACE_WITH_CLIENT_OPENAI_KEY
REACT_APP_ANTHROPIC_API_KEY=sk-ant-REPLACE_WITH_CLIENT_ANTHROPIC_KEY

# Security (CRITICAL: Generate secure random keys)
JWT_SECRET=REPLACE_WITH_STRONG_64_CHAR_JWT_SECRET_FOR_PRODUCTION
ENCRYPTION_KEY=REPLACE_WITH_32_CHAR_ENCRYPTION_KEY_FOR_PRODUCTION

# Database Configuration
DATABASE_URL=postgresql://ai_ide_user:REPLACE_WITH_DB_PASSWORD@postgres:5432/ai_ide
DATABASE_PASSWORD=REPLACE_WITH_SECURE_DB_PASSWORD

# Environment
NODE_ENV=production
PORT=3001
AI_SERVICE_PORT=3002

# API Configuration
VITE_API_BASE_URL=https://your-domain.com/api
API_TIMEOUT=30000

# Monitoring & Analytics (Production)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
ANALYTICS_ID=GA-XXXXXXXXX-X
GRAFANA_PASSWORD=REPLACE_WITH_SECURE_GRAFANA_PASSWORD

# Performance & Caching
REDIS_URL=redis://redis:6379
DATABASE_POOL_SIZE=20
MAX_REQUEST_SIZE=10mb
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# File Storage
MAX_UPLOAD_SIZE=100mb
UPLOAD_DIR=/app/uploads

# AI Configuration
OLLAMA_HOST=http://ollama:11434
QDRANT_URL=http://qdrant:6333
AI_MODEL_CACHE_SIZE=1000

# SSL/Security
FORCE_HTTPS=true
HELMET_ENABLED=true
CORS_ORIGIN=https://your-domain.com

# Backup Configuration
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=your-backup-bucket
AWS_ACCESS_KEY_ID=REPLACE_WITH_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=REPLACE_WITH_AWS_SECRET_KEY