# Production Environment Variables
# These will be set in Vercel dashboard

# Application URLs
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app
NEXT_PUBLIC_APP_ENV=production

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true
NEXT_PUBLIC_ENABLE_TELEMETRY=true

# Performance
NEXT_PUBLIC_CDN_URL=https://cdn.your-domain.com
NEXT_PUBLIC_STATIC_ASSETS_URL=https://assets.your-domain.com

# Support
NEXT_PUBLIC_SUPPORT_EMAIL=support@ottokode.ai
NEXT_PUBLIC_DOCS_URL=https://docs.ottokode.ai
NEXT_PUBLIC_STATUS_URL=https://status.ottokode.ai