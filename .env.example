# ============================================
# Branchcode AI - Environment Configuration
# ============================================

# ====== REQUIRED FOR PRODUCTION ======

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Stripe Configuration (for billing)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_publishable_key
STRIPE_SECRET_KEY=sk_live_your_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# App Configuration
VITE_APP_NAME="Branchcode AI"
VITE_APP_VERSION=1.0.0
VITE_APP_DOMAIN=https://branchcode.ai

# ====== OPTIONAL (Fallback AI Keys) ======
# Users can provide their own keys via the app UI
# These are fallback keys for trial users

VITE_OPENAI_API_KEY=sk-your_openai_key_here
VITE_ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_key_here
VITE_COHERE_API_KEY=your_cohere_key_here
VITE_MISTRAL_API_KEY=your_mistral_key_here

# ====== DEVELOPMENT SETTINGS ======
VITE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true

# ====== INFRASTRUCTURE ======
VITE_CDN_URL=https://cdn.branchcode.ai
VITE_API_BASE_URL=https://api.branchcode.ai

# Email Service (for invitations)
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your_sendgrid_api_key
FROM_EMAIL=noreply@branchcode.ai

# Analytics & Monitoring
VITE_POSTHOG_KEY=your_posthog_key
VITE_SENTRY_DSN=your_sentry_dsn

# Feature Flags
VITE_ENABLE_ENTERPRISE_FEATURES=true
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_AI_CHAT=true